#     "" u ($4/$5)*100 t "ABFT Recovery" lc rgb "#CC0066" 

set style data lines
set style fill solid 1.0 noborder
plot "dancer_1_error_timing_process_new_data" u ($1)/1000:(0):($2/$5)*100  w filledcu t "Load Checkpoint" lc rgb "#6600CC", \
     "" u ($1)/1000:($2/$5)*100:(($2+$3)/$5)*100  w filledcu t "Dump Checkpoint" lc rgb "#55CC55", \
     "" u ($1)/1000:(($2+$3)/$5)*100:(($2+$3+$4)/$5)*100 w filledcu t "ABFT Recovery" lc rgb "#CC0066" 





